<h1>Edit Book</h1>
<%= form_with model: @book, url: edit_book_path(@book), method: :patch, local: true do |f| %>
    <p>
    <%= f.label :title%><br>
    <%= f.text_field :title %>
    </p>
    <p>
    <%= f.label :published_year%><br>
    <%= f.text_field :published_year %>
    </p>
    <p>
    <%= f.label :author_id%><br>
    <%= f.text_field :author_id %>
    </p>
     <p>
      <%= f.submit 'Update' %>
    </p>
 <% end %>